{
  "dashboard": {
    "id": null,
    "title": "ThunderAI Dashboard",
    "panels": [
      {
        "type": "graph",
        "title": "CPU Usage",
        "targets": [
          {
            "expr": "thunderai_cpu_usage_percent",
            "legendFormat": "{{instance}}"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Model Accuracy",
        "targets": [
          {
            "expr": "thunderai_model_accuracy",
            "legendFormat": "{{model_name}} v{{version}}"
          }
        ]
      },
      {
        "type": "graph",
        "title": "RAM Usage",
        "targets": [
          {
            "expr": "thunderai_ram_usage_bytes",
            "legendFormat": "{{instance}}"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Prediction Latency",
        "targets": [
          {
            "expr": "thunderai_model_latency_seconds",
            "legendFormat": "{{model_name}} v{{version}}"
          }
        ]
      },
      {
        "type": "graph",
        "title": "Prediction Distribution",
        "targets": [
          {
            "expr": "thunderai_prediction_distribution",
            "legendFormat": "{{model_name}} - {{class}}"
          }
        ]
      }
    ]
  }
} 